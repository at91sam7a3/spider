export PATH=~/raspberrypi/tools/arm-rpi-linux-gnueabihf/bin:$PATH

conan install \
    .. \
    -g cmake_multi \
    -s arch="armv7" \
    -s build_type="Release" \
    -s compiler="gcc" \
    -s compiler.libcxx="libstdc++11" \
    -s os="Linux" \
    -s compiler.version=6 \
    -pr=rpi \
    -r spider_repo